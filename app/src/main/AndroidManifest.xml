<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="club.plus1.ec_electric">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/full_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.ECElectric"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name=".view.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.EnterActivity" />
        <activity android:name=".view.StartActivity" />
        <activity android:name=".view.RequestByCodeActivity" />
        <activity android:name=".view.RequestFromExcelActivity" />
        <activity android:name=".view.RequestTableActivity" />
        <activity android:name=".view.InvoiceTableActivity" />
        <activity android:name=".view.InvoiceDetailsActivity" />
    </application>

</manifest>